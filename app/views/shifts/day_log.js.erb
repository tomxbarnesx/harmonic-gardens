$("#add-form").empty().css("display", "none");
$("#day-log-container").empty();
<% if @shifts.length >= 1 %>
    $("#day-log-container").append("<%= j (render 'day_log_list') %>")
<% else %>
    $("#day-log-container").append("<%= j (render 'day_log_list_none') %>")
<% end %>

$("#day-log-container").css("display", "block");
